{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "A webpack starter for js",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 80",
    "test": "karma start",
    "dev": "rimraf dist && webpack --config config/webpack.dev.js --progress --profile --bail",
    "prod": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail"
