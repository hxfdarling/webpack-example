{
	"name": "webpack-starter",
	"version": "0.0.1",
	"description": "A webpack starter for js",
	"scripts": {
		"start": "webpack-dev-server --inline --progress",
		"test": "karma start",
		"build": "rimraf dist && webpack --config config/webpack.dev.js --watch --progress --profile --bail",
		"build:dev": "npm run build",
		"build:prod": "rimraf dist && webpack --config config/webpack.prod.js -p --progress --profile --bail"
	},
	"license": "MIT",
	"dependencies": {
		"core-js": "^2.4.1",
		"jquery": "^3.1.1",
		"rxjs": "5.0.0-beta.12",
		"vue": "^2.0.3",
		"vue-router": "^2.0.1",
		"zone.js": "^0.6.23"
	},
	"devDependencies": {
		"assets-webpack-plugin": "^3.4.0",
		"autoprefixer": "^6.5.1",
		"babel-core": "^6.0.0",
		"babel-loader": "^6.0.0",
		"babel-plugin-transform-runtime": "^6.0.0",
		"babel-preset-es2015": "^6.0.0",
		"babel-runtime": "^6.0.0",
		"copy-webpack-plugin": "^3.0.1",
		"css-loader": "^0.23.1",
		"extract-text-webpack-plugin": "^1.0.1",
		"file-loader": "^0.8.5",
		"html-loader": "^0.4.3",
		"html-webpack-plugin": "^2.15.0",
		"image-webpack-loader": "^2.0.0",
		"jasmine-core": "^2.4.1",
		"json-loader": "^0.5.4",
		"karma": "^1.2.0",
		"karma-jasmine": "^1.0.2",
		"karma-phantomjs-launcher": "^1.0.2",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-webpack": "^1.8.0",
		"move-to-parent-merging-webpack-plugin": "0.0.2",
		"null-loader": "^0.1.1",
		"phantomjs-prebuilt": "^2.1.7",
		"postcss-loader": "^0.13.0",
		"raw-loader": "^0.5.1",
		"readlines": "^0.2.0",
		"rimraf": "^2.5.2",
		"style-loader": "^0.13.1",
		"url-loader": "^0.5.7",
		"webpack": "^1.13.0",
		"webpack-dev-server": "^1.14.1",
		"webpack-merge": "^0.14.0"
	},
	"main": "webpack.config.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hxfdarling/webpack-starter.git"
	},
	"keywords": [
		"webpack",
		"webpack-starter",
		"vue"
	],
	"author": "zman",
	"bugs": {
		"url": "https://github.com/hxfdarling/webpack-starter/issues"
	},
	"homepage": "https://github.com/hxfdarling/webpack-starter#readme"
}